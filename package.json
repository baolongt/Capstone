  "main": "src/index.js",
  "scripts": {
    "prebuild": "rm -rf dist && npm run typecheck",
    "build": "vite build",
    "cleanup": "rm -rf node_modules && rm -rf package-lock.json",
    "dev": "vite --port 3000",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 50",
    "lint:fix": "npm run lint -- --fix",
    "precommit": "lint-staged",
    "prepare": "husky install",
    "prettier": "npx prettier --write ./src",
    "preview": "vite preview",
    "serve": "npm run build && vite preview --port 3000",
    "skip": "echo 'skip'",
    "prestart": "npm run typecheck",
    "start": "npm run dev",
    "typecheck": "tsc --noEmit"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts?(x)": [
      "npm run lint:fix"
    ],
    "package.json": "npx sort-package-json"
  },
